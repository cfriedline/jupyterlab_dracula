{
  "name": "@karosc/jupyterlab_dracula",
  "version": "2.1.0",
  "description": "Dracula Theme",
  "homepage": "https://github.com/karosc/jupyterlab_dracula",
  "bugs": {
    "url": "https://github.com/karosc/jupyterlab_dracula/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme"
  ],
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*",
    "static/*.css",
    "static/*.ttf",
    "static/*.eot",
    "static/*.woff",
    "static/*.woff2"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karosc/jupyterlab_dracula.git"
  },
  "scripts": {
    "build": "run-p build:**",
    "build:typescript": "tsc",
    "build:webpack": "webpack",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "watch": "run-p watch:**",
    "watch:typescript": "tsc -w",
    "watch:webpack": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "font-awesome": "~4.7.0"
  },
  "devDependencies": {
    "css-loader": "^3.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "~4.1.1",
    "rimraf": "~3.0.0",
    "svg-url-loader": "~3.0.3",
    "svgo": "^1.3.2",
    "svgo-loader": "~2.2.1",
    "typedoc": "^0.15.4",
    "typescript": "~3.7.3",
    "url-loader": "~3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themePath": "style/index.css"
  }
}
